extends /layouts/_default

block content
	.page
		.container
			.row
				.col-md-4
					h5 ICON
				.col-md-4
					h5 ICON
				.col-md-4
					h5 ICON

		.bg-3doers
			.container
				ul
					li.checked
						a(href='#') Review
						div
							p You have 24 hours to review and accept this order before it goes automatically back to the Workplace.
					li.checked
						a(href='#') Accepted
						div
							p You have 24 hours to review and accept this order before it goes automatically back to the Workplace.
					li.active
						a(href='#') Printing
						div
							p
								| You can now proceed with the printing, remember that a faster service will result in a higher rating for you.
								br
								| Once you’ve completed the order, please click on “ORDER PRINTED” to generate a shipping label.
					li
						a(href='#') Shipping
						div
							p You have 24 hours to review and accept this order before it goes automatically back to the Workplace.

				h4 Small_frog.stl
				.row
					.col-md-8
						div
							.row
								.col-md-4
									figure: img(src='data:image/svg+xml;base64,', style='width:100%')
								.col-md-8
									p
										strong COLOR: 
										| Blue
									p
										strong MATERIAL: 
										| ABS
									p
										strong AMOUNT: 
										| 2 pieces
									p
										strong VOLUME: 
										| 59.7 cm3
									p
										strong WEIGHT: 
										| 74 g
									p
										strong DIMENSIONS: 
										| 3.25 cm (W) 15.12 cm (H) 6.31(L)
					.col-md-4
						table
							tbody
								tr
									td Taxes:
									td 9.08 $
								tr
									td 3Doers:
									td 8.54 $
								tr
									td Shipping:
									td 8.45 $
								tr
									td Printer:
									td 25.62 $
				.row
					.col-md-8
						a(href='#') Download STF file
						br
					.col-md-4
						.form-group
							a.btn.btn-lg.btn-block.btn-green(href='#') ORDER PRINTED
						p: small Click the button above to generate a shipping label.

				.page-header
					h3 Conversation with project owner:

				.row
					.col-md-8
						.conversation
							.media
								.media-body.text-right
									p Would you consider resizing the object?
									p
										| By 
										strong Sam 
										| at 
										strong 11/Apr/2015 
										| at 
										strong 6:12 pm
								.media-right
									a(href='#')
										img.media-object(src='data:image/svg+xml;base64,')
							.media
								.media-left
									a(href='#')
										img.media-object(src='data:image/svg+xml;base64,')
								.media-body
									p Hi Sam, I’m sorry but that’s exactly the size I need.
									p
										| By 
										strong You 
										| at 
										strong 11/Apr/2015 
										| at 
										strong 8:25 pm
						form
							.form-group
								textarea.form-control(rows=4, placeholder='Write a comment')
							.text-right
								button.btn.btn-default(type='submit') Send
				br
				br