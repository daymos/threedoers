
extends ../base

block title
  |  Upload Project

block extra-headers
  meta(name="csrf-token", content="#{csrfToken}")

block body
  div
    .container
      .row
        .col-md-6
          h1.page-title
            span Upload
          h1.page-title
            span Project
      br
      br
      .row
        .col-md-6.col-md-offset-6
          .exclamation
            img(src='/img/exclamation_point.png')
          h1.page-title.right
            span File Must Be In
      .row
        .col-md-5.col-md-offset-7
          h1.page-title.right
            span Stl Format

      br
      br
      br
      .preview-print#clickable
        p#promptzone.upload
          | CLICK HERE TO CHOOSE
          br
          | FILE TO UPLOAD
        #result
        .progress.hide
          .progress-bar(aria-valuemax='100', aria-valuemin='0', aria-valuenow='0', role='progressbar')
            span.sr-only  0% Complete
      br
      br
      br
      br
      br
      br


    br
    br
    br
    include ../../includes/publicity
    br
    br
    br

block scripts
  script(src='/vendor/jquery.ajaxupload.js')
  script(src='/js/views/upload.js')

