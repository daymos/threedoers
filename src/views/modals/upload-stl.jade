block extra-headers
    meta(name="csrf-token", content="#{csrfToken}")

 #uploadSTLModal.modal.fade
  .modal-dialog
   .modal-content
    .modal-header
     .modal-title
         h4 A title
     .modal-body
      .preview-print#clickable-stl
        p#promptzone.upload
          img(src="http://fileinfo.com/images/icons/files/128/stl-2244.png")
        #result
        .progress.hide
          .progress-bar(aria-valuemax='100', aria-valuemin='0', aria-valuenow='0', role='progressbar')
            span.sr-only  0% Complete
