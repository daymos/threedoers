
extends ../base

block title
  |  Upload Project

block extra-headers
  meta(name="csrf-token", content="#{csrfToken}")

block body
  .section.primary
    .container#content
      h1.page-header Send to Print

      .preview-print#clickable-stl.col-md-8
        p#promptzone.upload
          | CLICK HERE TO CHOOSE
          br
          | FILE TO UPLOAD
        #result
        .progress.hide
          .progress-bar(aria-valuemax='100', aria-valuemin='0', aria-valuenow='0', role='progressbar')
            span.sr-only  0% Complete
      .col-md-4
          p Ciao

      br
      br
      br
block scripts
  script(src='/vendor/jquery.ajaxupload.js')
  script(src='/js/views/upload.js')

